{
  "name": "money-jar-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "accounting-js": "^1.1.1",
    "firebase": "^5.1.0",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "2.16.0",
    "react-slidedown": "^1.3.0",
    "recompose": "^0.27.1",
    "redux": "^4.0.0",
    "redux-form": "^7.4.2",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.9.2"
  },
  "scripts": {
    "start-ts": "react-scripts-ts start",
    "start": "npm-run-all -p watch-css start-ts",
    "build-ts": "react-scripts-ts build",
    "build": "npm-run-all build-css build-ts",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "build-css": "node-sass-chokidar src/styles/scss -o src/styles/css",
    "watch-css": "npm run build-css && node-sass-chokidar src/styles/scss -o src/styles/css --watch --recursive"
  },
  "devDependencies": {
    "@firebase/app-types": "^0.3.2",
    "@types/jest": "^23.1.1",
    "@types/node": "^10.3.4",
    "@types/react": "^16.4.1",
    "@types/react-dom": "^16.0.6",
    "@types/react-redux": "^6.0.2",
    "@types/react-router-dom": "^4.2.7",
    "@types/recompose": "^0.26.1",
    "@types/webpack-env": "^1.13.6",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "typescript": "^2.9.2"
  }
}
